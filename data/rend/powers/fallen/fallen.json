{
  "type": "origins:multiple",
  "summoner": {
    "type": "origins:entity_set"
  },
  "enemies": {
    "type": "origins:entity_set"
  },
  "chance_for_armour": {
    "type": "origins:action_on_callback",
    "entity_action_gained": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:chance",
          "chance": 0.4,
          "success_action": {
            "type": "origins:chance",
            "chance": 0.2,
            "success_action": {
              "type": "origins:execute_command",
              "command": "item replace entity @s armor.head with minecraft:iron_helmet 1"
            },
            "fail_action": {
              "type": "origins:execute_command",
              "command": "item replace entity @s armor.head with minecraft:chainmail_helmet 1"
            }
          }
        },
        {
          "type": "origins:chance",
          "chance": 0.4,
          "success_action": {
            "type": "origins:chance",
            "chance": 0.2,
            "success_action": {
              "type": "origins:execute_command",
              "command": "item replace entity @s armor.chest with minecraft:iron_chestplate 1"
            },
            "fail_action": {
              "type": "origins:execute_command",
              "command": "item replace entity @s armor.chest with minecraft:chainmail_chestplate 1"
            }
          }
        },
        {
          "type": "origins:chance",
          "chance": 0.4,
          "success_action": {
            "type": "origins:chance",
            "chance": 0.2,
            "success_action": {
              "type": "origins:execute_command",
              "command": "item replace entity @s armor.legs with minecraft:iron_leggings 1"
            },
            "fail_action": {
              "type": "origins:execute_command",
              "command": "item replace entity @s armor.legs with minecraft:chainmail_leggings 1"
            }
          }
        },
        {
          "type": "origins:chance",
          "chance": 0.4,
          "success_action": {
            "type": "origins:chance",
            "chance": 0.2,
            "success_action": {
              "type": "origins:execute_command",
              "command": "item replace entity @s armor.feet with minecraft:iron_boots 1"
            },
            "fail_action": {
              "type": "origins:execute_command",
              "command": "item replace entity @s armor.legs with minecraft:chainmail_boots 1"
            }
          }
        }
      ]
    }
  },
  "set_enemy_when_attacked": {
    "type": "origins:action_when_hit",
    "bientity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:target_action",
          "action": {
            "type": "origins:execute_command",
            "command": "scoreboard players operation #rend.attacked_fallen rend_team = @s rend_team"
          }
        },
        {
          "type": "origins:if_else",
          "condition": {
            "type": "origins:and",
            "conditions": [
              {
                "type": "origins:invert",
                "condition": {
                  "type": "origins:in_entity_set",
                  "set": "*:fallen/fallen_summoner",
                  "inverted": true
                }
              },
              {
                "type": "origins:actor_condition",
                "condition": {
                  "type": "origins:command",
                  "command": "execute if score #rend.attacked_fallen rend_team = @s rend_team",
                  "comparison": "==",
                  "compare_to": 0
                }
              }
            ]
          },
          "if_action": {
            "type": "origins:and",
            "actions": [
              {
                "type": "origins:invert",
                "action": {
                  "type": "add_to_entity_set",
                  "set": "*:fallen/fallen_enemies",
                  "time_limit": 3600
                }
              },
              {
                "type": "origins:actor_action",
                "action": {
                  "type": "origins:selector_action",
                  "selector": "@a",
                  "bientity_action": {
                    "type": "origins:invert",
                    "action": {
                      "type": "add_to_entity_set",
                      "set": "*:fallen/summoner_enemies",
                      "time_limit": 3600
                    }
                  },
                  "bientity_condition": {
                    "type": "origins:target_condition",
                    "condition": {
                      "type": "origins:command",
                      "command": "execute if score #rend.attacked_fallen rend_team = @s rend_team",
                      "comparison": "==",
                      "compare_to": 1
                    }
                  }
                }
              },
              {
                "type": "origins:actor_action",
                "action": {
                  "type": "origins:area_of_effect",
                  "radius": 20,
                  "shape": "sphere",
                  "bientity_action": {
                    "type": "origins:damage",
                    "damage_type": "generic",
                    "amount": 0
                  },
                  "bientity_condition": {
                    "type": "origins:and",
                    "conditions": [
                      {
                        "type": "origins:target_condition",
                        "condition": {
                          "type": "origins:and",
                          "conditions": [
                            {
                              "type": "origins:entity_type",
                              "entity_type": "minecraft:skeleton"
                            },
                            {
                              "type": "origins:command",
                              "command": "execute if score #rend.attacked_fallen rend_team = @s rend_team",
                              "comparison": "==",
                              "compare_to": 1
                            }
                          ]
                        }
                      }
                    ]
                  }
                }
              }
            ]
          }
        }
      ]
    },
    "cooldown": 100
  },
  "prevent_damaging_non_enemies": {
    "type": "origins:modify_damage_dealt",
    "bientity_condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:in_entity_set",
          "set": "*:fallen/fallen_enemies",
          "inverted": true
        },
        {
          "type": "origins:target_condition",
          "condition": {
            "type": "origins:and",
            "conditions": [
              {
                "type": "origins:entity_type",
                "entity_type": "minecraft:skeleton"
              },
              {
                "type": "origins:nbt",
                "nbt": "{Tags: ['rend_fallen']}"
              }
            ]
          },
          "inverted": true
        }
      ]
    },
    "modifier": {
      "id": "rend:fallen/fallen_prevent_damaging_non_enemies",
      "operation": "set_total",
      "amount": -1
    }
  },
  "prevent_friendly_damage": {
    "type": "origins:modify_damage_taken",
    "bientity_condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:target_condition",
          "condition": {
            "type": "origins:command",
            "command": "function rend:fallen/record_attacked_team",
            "comparison": "==",
            "compare_to": 1
          }
        },
        {
          "type": "origins:actor_condition",
          "condition": {
            "type": "origins:entity_type",
            "entity_type": "minecraft:player",
            "inverted": true
          }
        },
        {
          "type": "origins:actor_condition",
          "condition": {
            "type": "origins:command",
            "command": "execute if score #rend_attacked_fallen rend_team = @s rend_team",
            "comparison": "==",
            "compare_to": 1
          }
        }
      ]
    },
    "modifier": {
      "id": "rend:fallen/fallen_prevent_friendly_damage",
      "operation": "set_total",
      "amount": -1
    }
  },
  "weapon_switch_cooldown": {
    "type": "origins:cooldown",
    "cooldown": 20
  },
  "weapon_switch": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "execute store result storage minecraft:rend fallen.counter int 1 run scoreboard players get @s rend_team"
        },
        {
          "type": "origins:if_else",
          "condition": {
            "type": "origins:entity_in_radius",
            "radius": 20,
            "shape": "sphere",
            "bientity_condition": {
              "type": "origins:and",
              "conditions": [
                {
                  "type": "origins:in_entity_set",
                  "set": "*:fallen/fallen_enemies"
                },
                { "type": "origins:can_see" },
                {
                  "type": "target_condition",
                  "condition": {
                    "type": "origins:status_effect",
                    "effect": "minecraft:invisibility"
                  },
                  "inverted": true
                }
              ]
            },
            "comparison": ">",
            "compare_to": 0
          },
          "if_action": {
            "type": "origins:if_else",
            "condition": {
              "type": "origins:and",
              "conditions": [
                {
                  "type": "origins:resource",
                  "resource": "*:fallen/fallen_weapon_switch_cooldown",
                  "comparison": "==",
                  "compare_to": 0
                },
                {
                  "type": "origins:entity_in_radius",
                  "radius": 4,
                  "shape": "sphere",
                  "bientity_condition": {
                    "type": "origins:and",
                    "conditions": [
                      {
                        "type": "origins:in_entity_set",
                        "set": "*:fallen/fallen_enemies"
                      },
                      { "type": "origins:can_see" }
                    ]
                  },
                  "comparison": ">",
                  "compare_to": 0
                }
              ]
            },
            "if_action": {
              "type": "origins:if_else",
              "condition": {
                "type": "origins:equipped_item",
                "equipment_slot": "mainhand",
                "item_condition": {
                  "type": "origins:ingredient",
                  "ingredient": {
                    "item": "minecraft:iron_sword"
                  },
                  "inverted": true
                }
              },
              "if_action": {
                "type": "origins:and",
                "actions": [
                  {
                    "type": "origins:trigger_cooldown",
                    "power": "*:fallen/fallen_weapon_switch_cooldown"
                  },
                  {
                    "type": "origins:execute_command",
                    "command": "item replace entity @s weapon.mainhand with minecraft:iron_sword[enchantments={\"minecraft:sharpness\":3s}] 1"
                  },
                  {
                    "type": "origins:play_sound",
                    "sound": "minecraft:item.armor.equip_iron",
                    "category": "hostile"
                  }
                ]
              }
            },
            "else_action": {
              "type": "origins:if_else",
              "condition": {
                "type": "origins:equipped_item",
                "equipment_slot": "mainhand",
                "item_condition": {
                  "type": "origins:ingredient",
                  "ingredient": {
                    "item": "minecraft:bow"
                  },
                  "inverted": true
                }
              },
              "if_action": {
                "type": "origins:and",
                "actions": [
                  {
                    "type": "origins:trigger_cooldown",
                    "power": "*:fallen/fallen_weapon_switch_cooldown"
                  },
                  {
                    "type": "origins:execute_command",
                    "command": "item replace entity @s weapon.mainhand with minecraft:bow[enchantments={\"minecraft:power\":3s}] 1"
                  },
                  {
                    "type": "origins:play_sound",
                    "sound": "minecraft:item.armor.equip_elytra",
                    "category": "hostile"
                  }
                ]
              }
            }
          },
          "else_action": {
            "type": "origins:if_else",
            "condition": {
              "type": "origins:equipped_item",
              "equipment_slot": "mainhand",
              "item_condition": {
                "type": "origins:ingredient",
                "ingredient": {
                  "item": "minecraft:air"
                },
                "inverted": true
              }
            },
            "if_action": {
              "type": "origins:and",
              "actions": [
                {
                  "type": "origins:execute_command",
                  "command": "item replace entity @s weapon.mainhand with minecraft:air 1"
                },
                {
                  "type": "origins:play_sound",
                  "sound": "minecraft:item.armor.equip_leather",
                  "category": "hostile"
                }
              ]
            }
          }
        }
      ]
    },
    "interval": 20
  },
  "effect_on_death": {
    "type": "origins:action_on_death",
    "bientity_action": {
      "type": "origins:target_action",
      "action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:execute_command",
            "command": "particle minecraft:soul ~ ~1 ~ .1 .3 .1 0.05 10"
          },
          {
            "type": "origins:play_sound",
            "sound": "minecraft:particle.soul_escape",
            "category": "hostile"
          }
        ]
      }
    }
  }
}
